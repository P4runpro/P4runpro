@ dippool 1024
@ portpool  1024
program loadbanlancer(<hdr.ipv4.dst, 7.0.0.0, 0xffffff00>) {
    HASH_5_TUPLE_MEM(portpool);
    MEMREAD(portpool);
    BRANCH:
    case(<sar, 0, 0xfffffff>) {
        FORWARD(0);
    }
    case(<sar, 1, 0xfffffff>) {
        FORWARD(1);
    };
    MEMREAD(dippool);
    MODIFY(hdr.ipv4.dst, sar);
}